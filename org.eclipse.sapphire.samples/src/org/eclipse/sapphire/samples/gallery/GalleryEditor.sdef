                                </action>
                                <action-handler>
                                    <action>Sapphire.Sample.Prefix</action>
                                    <impl>PrefixActionHandlerForList</impl>
                                    <param>
                                        <name>prefix</name>
                                        <value>Prefix</value>
                                    </param>
                                </action-handler>
                                <child-property>
                                    <property>Reference</property>
                                    <action>
                                        <id>Sapphire.Sample.Prefix</id>
